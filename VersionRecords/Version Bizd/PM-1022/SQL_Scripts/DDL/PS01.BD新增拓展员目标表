  /*  Database name `bizd`  拓展员目标表*/
  use bizd;
  /*
  1、预估表容量:拓展系统-拓展员目标表，基础数据新增较少
  2、每次读取量:在20条左右
  3、主要查询:
    SELECT id,servicePersonId, activeHouseCount, activeLandlordCount,targetMonth,orgId
    FROM
      bizd_serviceperson_target
    WHERE id = 101
  4. 索引:目前索引默认主键id
  */
CREATE TABLE `bizd_serviceperson_target` (
  `id` int(11) NOT NULL COMMENT '主键',
  `servicePersonId` int(11) DEFAULT NULL COMMENT '服务人ID',
  `activeHouseCount` int(5) DEFAULT NULL COMMENT '激活房源数',
  `activeLandlordCount` int(5) DEFAULT NULL COMMENT '激活房东数',
  `targetMonth` int(6) DEFAULT NULL COMMENT '目标月份',
  `orgId` int(11) DEFAULT NULL COMMENT '组织机构ID',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COMMENT='拓展员目标表'

